{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "secondary_prompt": {
    "template": "... ",
    "foreground": "#ffffff",
    "background": "transparent"
  },
  "console_title_template": "{{ .Shell|lower }}",
  "blocks": [
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "style": "plain",
          "template": "\u250c {{ .UserName }}@{{ .HostName }} ",
          "foreground": "#757575",
          "background": "",
          "type": "session"
        },
        {
          "properties": {
            "style": "agnoster_short"
          },
          "style": "powerline",
          "template": "<i> {{ abbrev 100 .Path }} </i>",
          "foreground": "#ffffff",
          "powerline_symbol": "\ue0b0",
          "background": "#757575",
          "type": "path"
        }
      ]
    },
    {
      "type": "prompt",
      "alignment": "right",
      "segments": [
        {
          "properties": {
            "style": "austin",
            "threshold": 0
          },
          "style": "plain",
          "template": " <#757575,>{{ .FormattedMs }}</>",
          "foreground": "#ffffff",
          "background": "",
          "type": "executiontime"
        },
        {
          "style": "plain",
          "template": " {{ .Name }}",
          "foreground": "#007acc",
          "background": "",
          "type": "shell"
        },
        {
          "style": "plain",
          "template": " <#757575,>as</> root",
          "foreground": "#ff5722",
          "background": "",
          "type": "root"
        },
        {
          "properties": {
            "time_format": "15:04"
          },
          "style": "plain",
          "template": " <#757575,>|</> {{ .CurrentDate | date .Format }}",
          "foreground": "#689f38",
          "background": "",
          "type": "time"
        },
        {
          "properties": {
            "branch_ahead_icon": "\u2191",
            "branch_behind_icon": "\u2193",
            "branch_gone": "\u2262",
            "branch_icon": "\ue0a0 ",
            "branch_identical_icon": "\u2261",
            "cherry_pick_icon": "âœ“ ",
            "commit_icon": "\u25b7 ",
            "fetch_stash_count": true,
            "fetch_status": true,
            "merge_icon": "\u25f4 ",
            "no_commits_icon": "[no commits]",
            "rebase_icon": "\u2c62 ",
            "tag_icon": "\u25b6 "
          },
          "style": "plain",
          "template": " <#757575,>|</> {{ abbrev 20 .RepoName }} <#757575,>|</> {{ .HEAD }} ",
          "foreground": "#f0a637",
          "background": "",
          "type": "git",
          "foreground_templates": [
            "{{ if or (.Working.Changed) (.Staging.Changed) }}#ff9248{{ end }}",
            "{{ if and (gt .Ahead 0) (gt .Behind 0) }}#f26d50{{ end }}",
            "{{ if gt .Ahead 0 }}#89d1dc{{ end }}",
            "{{ if gt .Behind 0 }}#c5b6ad{{ end }}"
          ]
        }
      ]
    },
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "style": "plain",
          "template": "\u2514",
          "foreground": "#757575",
          "background": "",
          "type": "text"
        },
        {
          "style": "plain",
          "template": " ({{ .Venv }})",
          "foreground": "#29ffa1",
          "background": "",
          "type": "python"
        },
        {
          "style": "plain",
          "template": " $",
          "foreground": "#ffffff",
          "background": "",
          "type": "text"
        }
      ],
      "newline": true
    }
  ],
  "version": 3,
  "final_space": true
}
